
# Output directory for analysis results
OUT_DIR=./outputs

# ONNX Model path (optional - will use fallback if not found)
ONNX_MODEL_PATH=resnet18_gazevec_best.onnx

# CORS settings
ALLOWED_ORIGINS=http://localhost,http://127.0.0.1,http://localhost:3000,http://localhost:5173

# Server port
PORT=8002

# ONNX Runtime settings
ORT_INTRA_THREADS=2
ORT_INTER_THREADS=1

# Behavior analysis settings
MAX_BUFFER=120
GAZE_SMOOTHING_WINDOW=5
EAR_THRESHOLD=0.25
GAZE_THRESHOLD_DEG=15.0
ENGAGEMENT_WINDOW_S=60
SESSION_TTL_S=1800
WS_FRAME_RATE_LIMIT=15.0
FRAME_SAMPLING_INTERVAL_S=3.0
